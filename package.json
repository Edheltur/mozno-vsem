{
  "name": "mozno-vsem",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "prettier --check .",
    "pretty": "prettier --write .",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@storeon/localstorage": "1.2.0",
    "classnames": "2.2.6",
    "faunadb": "2.14.2",
    "grommet": "2.15.2",
    "http-status-codes": "1.4.0",
    "lyam": "1.3.0",
    "next": "9.5.5",
    "next-auth": "3.1.0",
    "node-fetch": "2.6.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "storeon": "3.0.4",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.10",
    "@types/next-auth": "3.1.16",
    "@types/node": "14.11.10",
    "@types/node-fetch": "2.5.7",
    "@types/react": "16.9.53",
    "@types/styled-components": "5.1.4",
    "husky": "4.3.0",
    "next-transpile-modules": "4.1.0",
    "prettier": "2.1.2",
    "pretty-quick": "2.0.2",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
